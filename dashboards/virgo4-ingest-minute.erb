<% content_for :title do %><%= "Virgo4 Ingest, previous #{CONFIG['INGEST_GENERAL']['last_n_minutes']} minutes (#{ENV[ 'ENVIRONMENT' ]})" %><% end %>
<div class="gridster">
   <center><div style="font-size: 22px"><a href="/virgo4-pools" class="pull-left">&nbsp;previous</a><%= "Virgo4 Ingest, previous #{CONFIG['INGEST_GENERAL']['last_n_minutes']} minutes (#{ENV[ 'ENVIRONMENT' ]})" %><a href="/virgo4-ingest-hour" class="pull-right">next&nbsp;</a></div></center>

  <ul>

    <% services = [ 'INGEST_SIRSI_MARC_CONVERT_QUEUE_IN',
                    'INGEST_HATHI_MARC_CONVERT_QUEUE_IN',
                    'INGEST_TRACKSYS_ENRICH_QUEUE_IN',
                    'INGEST_SOLR_UPDATE_QUEUE_IN',
                    'INGEST_SOLR_DELETE_QUEUE_IN',
                    'INGEST_CACHE_QUEUE_IN' ] %>
     <% services.each do |key| %>

        <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
           <div data-id="<%= "#{CONFIG[key]['id']}-minute" %>" data-view="Meter" data-title="<%= "#{CONFIG[key]['title']}" %>" data-min="0" data-max="2000000" ></div>
        </li>

     <% end %>

    <% services = [ 'INGEST_SIRSI_MARC_CONVERT_QUEUE_OUT',
                    'INGEST_HATHI_MARC_CONVERT_QUEUE_OUT',
                    'INGEST_TRACKSYS_ENRICH_QUEUE_OUT',
                    'INGEST_SOLR_UPDATE_QUEUE_OUT',
                    'INGEST_SOLR_DELETE_QUEUE_OUT',
                    'INGEST_CACHE_QUEUE_OUT' ] %>
    <% services.each do |key| %>

      <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
        <div data-id="<%= "#{CONFIG[key]['id']}-minute" %>" data-view="Meter" data-title="<%= "#{CONFIG[key]['title']}" %>" data-min="0" data-max="2000000" %></div>
      </li>

    <% end %>

  </ul>
</div>