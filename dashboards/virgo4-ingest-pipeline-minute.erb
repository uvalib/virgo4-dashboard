<% content_for :title do %><%= "Virgo4 Ingest Pipeline, previous #{CONFIG['INGEST_GENERAL']['last_n_minutes']} minutes (#{ENV[ 'ENVIRONMENT' ]})" %><% end %>
<div class="gridster">
   <center><div style="font-size: 22px"><a href="/virgo4-ingest-inbound-hour" class="pull-left">&nbsp;previous</a><%= "Virgo4 Ingest Pipeline, previous #{CONFIG['INGEST_GENERAL']['last_n_minutes']} minutes (#{ENV[ 'ENVIRONMENT' ]})" %><a href="/virgo4-ingest-pipeline-hour" class="pull-right">next&nbsp;</a></div></center>

  <ul>

    <% services = [ 'INGEST_TRACKSYS_ENRICH_QUEUE_IN',
                    'INGEST_DEFAULT_SOLR_UPDATE_QUEUE_IN',
                    'INGEST_IMAGE_SOLR_UPDATE_QUEUE_IN',
                    'INGEST_IIIF_CACHE_QUEUE_IN',
                    'INGEST_CACHE_QUEUE_IN' ] %>
     <% services.each do |key| %>

        <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
           <div data-id="<%= "#{CONFIG[key]['id']}-minute" %>" data-view="Meter" data-title="<%= "#{CONFIG[key]['title']}" %>" data-min="0" data-max="5000000" ></div>
        </li>

     <% end %>

    <% 1.times do || %>
      <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
        <div data-id="blank" data-view="Comments" data-title="Blank" data-text=""></div>
      </li>
    <% end %>

    <% services = [ 'INGEST_TRACKSYS_ENRICH_QUEUE_OUT',
                    'INGEST_DEFAULT_SOLR_UPDATE_QUEUE_OUT',
                    'INGEST_IMAGE_SOLR_UPDATE_QUEUE_OUT',
                    'INGEST_IIIF_CACHE_QUEUE_OUT',
                    'INGEST_CACHE_QUEUE_OUT' ] %>
    <% services.each do |key| %>

      <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
        <div data-id="<%= "#{CONFIG[key]['id']}-minute" %>" data-view="Meter" data-title="<%= "#{CONFIG[key]['title']}" %>" data-min="0" data-max="5000000" %></div>
      </li>

    <% end %>

    <% 1.times do || %>
      <li data-row="1" data-col="1" data-sizex="1" data-sizey="1">
        <div data-id="blank" data-view="Comments" data-title="Blank" data-text=""></div>
      </li>
    <% end %>

  </ul>
</div>